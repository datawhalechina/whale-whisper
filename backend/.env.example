# Example environment for WhaleWhisper backend

ENGINE_CONFIG_PATH=./config/engines.yaml
WS_AUTH_TOKEN=

LLM_PROVIDER=openai_compat
OPENAI_BASE_URL=https://api.openai.com/v1
OPENAI_API_KEY=your_key_here
OPENAI_MODEL=gpt-4o-mini
LLM_TEMPERATURE=0.2
LLM_TIMEOUT=60

# Dify
DIFY_BASE_URL=http://localhost/v1
DIFY_API_KEY=your_key_here
DIFY_USER=whale

# FastGPT
FASTGPT_BASE_URL=https://cloud.fastgpt.cn/api
FASTGPT_API_KEY=your_key_here
FASTGPT_UID=whale

# Coze
COZE_API_BASE=https://api.coze.cn
COZE_TOKEN=your_token_here
COZE_BOT_ID=your_bot_id_here
COZE_USER=whale

# Engine API keys (used by config/engines.yaml)
OPENROUTER_API_KEY=your_key_here
DEEPSEEK_API_KEY=your_key_here
AI302_API_KEY=your_key_here
GROQ_API_KEY=your_key_here
GLM_API_KEY=your_key_here
